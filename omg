local function callback(Text)
 if Text == "Posted on scriptblox.com" then
  setclipboard("scriptblox.com")
elseif Text == ("sub here plz") then
 setclipboard("https://www.youtube.com/channel/UCYmzvg4P3hGHLMCKs5yCOig")
 end
end
 
local NotificationBindable = Instance.new("BindableFunction")
NotificationBindable.OnInvoke = callback
