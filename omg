game.StarterGui:SetCore("SendNotification",  {
 Title = "yeah made by windows support";
 Text = "click on any sh u want";
 Icon = "";
 Duration = 30;
 Button1 = "scriptblox.com";
 Button2 = "YT";
 Callback = NotificationBindable;
})
